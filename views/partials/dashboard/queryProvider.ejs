<div class="modal fade" id="provider-modal" tabindex="-1" role="dialog">
  <div class="modal-dialog">
    <div class="modal-content">
      <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
        <h4 class="modal-title">Select Hospital</h4>
      </div>
      <div class="modal-body">
        <!-- Form -->
        <form action="dashboard/q-provider" method="POST">
          <div class="form-group">
            <label for="provider">CMS Provider ID*</label>
            <input id="provider" class="form-control" type="number" name="provider" required>
            <p>* Required - 6-digit provider id supplied by the Center for Medicare and Medicaid Services(CMS)</p>
          </div>

          <div class="modal-footer">
          <input class="btn btn-primary" type="submit">
          </div>
        </form><!-- end form -->
      </div><!-- end modal-body -->
    </div><!-- /.modal-content -->
  </div><!-- /.modal-dialog -->
</div><!-- /.modal -->
