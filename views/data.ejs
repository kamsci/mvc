<!-- Header -->
<ul class="enter-nav">
  <li class="list-nav"><a href="/">Home</a></li>
  <li class="list-nav">logout</li>
</ul>
<h6 class="list-name">Bobby</h6>
<!-- Select Data Set-->
<div class="container-fluid">
  <div>
    <form class="form-inline" action="/dashboard/q-dataset" method="POST">
      <div class="form-group">
        <h5 for="dataset" class="inline">Select Data Set From Library </h5>
          <select id="dataset" class="form-control dropdown" name="library">
            <!-- forEach loop with datasets in database -->
            <% userData.forEach(function(data) { %>
              <option><%= data.name %></option>
            <% }) %>
          </select>
        </div>
      <input type="submit" value="Run Report" class="btn inline">
    </form>
  </div>
  <div>
  </div>
  <hr class="thick">
  <!-- Data Set List -->
  <div>
    <h4 class="header">My Data Set Library</h4>
    <a class="btn-floating btn-large waves-effect waves-light red" data-toggle="modal" data-target="#dataset-modal"><i class="material-icons">add</i></a>
    <h6 class="inline">Add dashboard dataset</h6>
    <!-- <button type="button" class="btn btn-info btn-xs header-btn" data-toggle="modal" data-target="#dataset-modal">Add dashboard dataset</button> -->
  </div>
  <!-- forEach loop here -->
  <% userData.forEach(function(data) { %>
    <div>
      <h5 class="report"><%= data.name %></h5>
    </div>
      <!-- Provider info -->
    <div class="dataset-box"> <!-- Dataset -->
      <div>
        <h6 class="header">Hospital Provider</h6>
      </div>
      <div>
        <table class="responsive-table data-table">
          <thead>
            <tr>
              <th>Provider ID</th>
              <th>Name</th>
              <th>State</th>
              <th>City</th>
              <th>Ownership</th>
              <th>Type</th>
              <th>Emergency Services</th>
            </tr>
          </thead>
          <tbody>
            <tr>
              <td><%= data.hospital.provider_id %></td>
              <td><%= data.hospital.hospital_name %></td>
              <td><%= data.hospital.state %></td>
              <td><%= data.hospital.city %></td>
              <td><%= data.hospital.hospital_ownership %></td>
              <td><%= data.hospital.hospital_type %></td>
              <td><%= data.hospital.emergency_services %></td>
            </tr>
          </tbody>
        </table>
      </div>
      <!-- Benchmarks info -->
      <div>
        <h6 class="header">Compared against benchmark: </h6>
        <h6 class="inline"><%= data.benchmark.name %></h6>
      </div>
    </div><!-- end dataset -->
    <hr>
  <% }) %><!-- %>end for loop -->
</div>

<!-- Include New Data Set modal -->
<%- include partials/modals/newDataset.ejs %>
